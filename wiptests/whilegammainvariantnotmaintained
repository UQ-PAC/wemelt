_global var z:
_L: TRUE
_R_var: (TRUE, z' == z)

_local var secret

_global var q:
_L: FALSE
_R_var: (TRUE, q' == q)

_P_inv: TRUE
_G: z' >= z
_P_0: z == 0
_Gamma_0: secret -> FALSE, q -> TRUE

while(TRUE)
_invariant: TRUE
_Gamma: z -> TRUE, secret -> FALSE, q -> TRUE
{
    q = z;
    q = secret;
}

